        </Field>
        <Field Name="obe_weight">
          <DataField>obe_weight</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="clo">
          <DataField>clo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="clo_achieved">
          <DataField>clo_achieved</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
